<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Firestorm.EntityFramework6</id>
    <version>$(FsVersion)</version>
    <title>Firestorm with Entity Framework 6</title>
    <description>Use Entity Framework 6 as a data source for Firestorm APIs</description>
    <tags>firestorm rest api entityframework orm ef6</tags>

    <authors>Connell Watkins</authors>
    <copyright>Copyright Â© Connell Watkins 2017</copyright>
    <projectUrl>https://github.com/connellw/Firestorm</projectUrl>
    <licenseUrl>https://github.com/connellw/Firestorm/blob/master/LICENSE.txt</licenseUrl>
    <iconUrl>https://github.com/connellw/Firestorm/raw/master/nuget/icon.png</iconUrl>

    <dependencies>
      <dependency id="Firestorm.Data" version="$(FsVersion)" />
      <dependency id="EntityFramework" version = "6.1.3" />
    </dependencies>
    
    <references>
      <reference file="Firestorm.EntityFramework6.dll" />
    </references>
  </metadata>
  <files>
    <file src="..\..\src\Firestorm.EntityFramework6\bin\$(FsConfiguration)\net452\*.dll" target="lib\net452\"  />
  </files>
</package>
