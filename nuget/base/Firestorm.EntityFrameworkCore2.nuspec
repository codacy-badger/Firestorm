<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Firestorm.EntityFrameworkCore2</id>
    <version>$(FsVersion)</version>
    <title>Firestorm with Entity Framework Core 2.0</title>
    <description>Use Entity Framework Core as a data source for Firestorm APIs</description>
    <tags>firestorm rest api entityframework efcore orm</tags>

    <authors>Connell Watkins</authors>
    <copyright>Copyright Â© Connell Watkins 2017</copyright>
    <projectUrl>https://github.com/connellw/Firestorm</projectUrl>
    <licenseUrl>https://github.com/connellw/Firestorm/blob/master/LICENSE.txt</licenseUrl>
    <iconUrl>https://github.com/connellw/Firestorm/raw/master/nuget/icon.png</iconUrl>

    <dependencies>
      <dependency id="Firestorm.Data" version="$(FsVersion)" />
      <dependency id="Microsoft.EntityFrameworkCore.SqlServer" version="2.0.0" />
    </dependencies>
    
    <references>
      <reference file="Firestorm.EntityFrameworkCore2.dll" />
    </references>
  </metadata>
  <files>
    <file src="..\..\src\Firestorm.EntityFrameworkCore2\bin\$(FsConfiguration)\netstandard2.0\*.dll" target="lib\netstandard2.0\"  />
