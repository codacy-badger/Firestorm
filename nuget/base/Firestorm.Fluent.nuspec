<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Firestorm.Fluent</id>
    <version>$(FsVersion)</version>
    <title>All the Firestorm Fluent API packages</title>
    <description>Build REST APIs using the Firestorm Fluent API.</description>
    <tags>firestorm rest api fluent</tags>

    <authors>Connell Watkins</authors>
    <copyright>Copyright Â© Connell Watkins 2018</copyright>
    <projectUrl>https://github.com/connellw/Firestorm</projectUrl>
    <licenseUrl>https://github.com/connellw/Firestorm/blob/master/LICENSE.txt</licenseUrl>
    <iconUrl>https://github.com/connellw/Firestorm/raw/master/nuget/icon.png</iconUrl>

    <dependencies>
      <dependency id="Firestorm.Core" version="$(FsVersion)" />
    </dependencies>
    
    <references>
      <reference file="Firestorm.Fluent.dll" />
    </references>
  </metadata>
  <files>
    <file src="..\..\src\Firestorm.Fluent\bin\$(FsConfiguration)\net452\*.dll" target="lib\net452\"  />
    <file src="..\..\src\Firestorm.Fluent\bin\$(FsConfiguration)\netstandard2.0\*.dll" target="lib\netstandard2.0\"  />
  </files>
</package>
