<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Firestorm.Fluent.Start</id>
    <version>$(FsVersion)</version>
    <title>Firestorm Fluent Start</title>
    <description>Ties in Endpoints by providing a StartResourceFactory for the Fluent API.</description>
    <tags>firestorm rest api fluent endpoints start</tags>

    <authors>Connell Watkins</authors>
    <copyright>Copyright Â© Connell Watkins 2018</copyright>
    <projectUrl>https://github.com/connellw/Firestorm</projectUrl>
    <licenseUrl>https://github.com/connellw/Firestorm/blob/master/LICENSE.txt</licenseUrl>
    <iconUrl>https://github.com/connellw/Firestorm/raw/master/nuget/icon.png</iconUrl>

    <dependencies>
      <dependency id="Firestorm.Core" version="$(FsVersion)" />
      <dependency id="Firestorm.Endpoints" version="$(FsVersion)" />
      <dependency id="Firestorm.Fluent" version="$(FsVersion)" />
    </dependencies>
    
    <references>
      <reference file="Firestorm.Fluent.Fuel.dll" />
      <reference file="Firestorm.Fluent.Start.dll" />
    </references>
  </metadata>
  <files>
    <file src="..\..\src\Firestorm.Fluent.Start\bin\$(FsConfiguration)\net45\*.dll" target="lib\net45\"  />
    <file src="..\..\src\Firestorm.Fluent.Start\bin\$(FsConfiguration)\netstandard2.0\*.dll" target="lib\netstandard2.0\"  />
  </files>
</package>