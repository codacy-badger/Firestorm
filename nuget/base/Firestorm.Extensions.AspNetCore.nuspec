<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Firestorm.Extensions.AspNetCore</id>
    <version>$(FsVersion)</version>
    <title>Firestorm Service Extensions for ASP.NET Core</title>
    <description>ASP.NET Core middleware and services extensions for Firestorm</description>
    <tags>firestorm rest api aspnetcore extensions</tags>

    <authors>Connell Watkins</authors>
    <copyright>Copyright Â© Connell Watkins 2018</copyright>
    <projectUrl>https://github.com/connellw/Firestorm</projectUrl>
    <licenseUrl>https://github.com/connellw/Firestorm/blob/master/LICENSE.txt</licenseUrl>
    <iconUrl>https://github.com/connellw/Firestorm/raw/master/nuget/icon.png</iconUrl>

    <dependencies>
      <dependency id="Firestorm.Core" version="$(FsVersion)" />
      <dependency id="Firestorm.AspNetCore2" version="$(FsVersion)" />
      <dependency id="Firestorm.EntityFrameworkCore2" version="$(FsVersion)" />
      <dependency id="Firestorm.Stems.Roots.DataSource" version="$(FsVersion)" />
      <dependency id="Firestorm.Fluent.Start" version="$(FsVersion)" />
      <dependency id="Newtonsoft.Json" version="10.0.1" />
      <dependency id="Microsoft.AspNetCore.Http" version="2.0.0" />
    </dependencies>
    
    <references>
      <reference file="Firestorm.Extensions.AspNetCore.dll" />
    </references>
  </metadata>
  <files>
    <file src="..\..\src\Firestorm.Extensions.AspNetCore\bin\$(FsConfiguration)\netstandard2.0\*.dll" target="lib\netstandard2.0\"  />
  </files>
</package>
