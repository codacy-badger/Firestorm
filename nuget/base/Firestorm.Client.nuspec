<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Firestorm.Client</id>
    <version>$(FsVersion)</version>
    <title>Firestorm Client</title>
    <description>A .NET REST client that implements Firestorm Core.</description>
    <tags>firestorm rest api client</tags>
    
    <authors>Connell Watkins</authors>
    <copyright>Copyright Â© Connell Watkins 2017</copyright>
    <projectUrl>https://github.com/connellw/Firestorm</projectUrl>
    <licenseUrl>https://github.com/connellw/Firestorm/blob/master/LICENSE.txt</licenseUrl>
    <iconUrl>https://github.com/connellw/Firestorm/raw/master/nuget/icon.png</iconUrl>

    <dependencies>
      <dependency id="Firestorm.Core" version="$(FsVersion)" />
    </dependencies>
    
    <references>
      <reference file="Firestorm.Client.dll" />
    </references>
  </metadata>
  <files>
    <file src="..\..\src\Firestorm.Client\bin\$(FsConfiguration)\net45\*.dll" target="lib\net45\"  />
    <file src="..\..\src\Firestorm.Client\bin\$(FsConfiguration)\netstandard2.0\*.dll" target="lib\netstandard2.0\"  />
  </files>
</package>