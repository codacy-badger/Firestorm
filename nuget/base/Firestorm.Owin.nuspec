<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Firestorm.Owin</id>
    <version>$(FsVersion)</version>
    <title>Firestorm for OWIN</title>
    <description>OWIN middleware and helpers for Firestorm</description>
    <tags>firestorm rest api owin</tags>

    <authors>Connell Watkins</authors>
    <copyright>Copyright Â© Connell Watkins 2017</copyright>
    <projectUrl>https://github.com/connellw/Firestorm</projectUrl>
    <licenseUrl>https://github.com/connellw/Firestorm/blob/master/LICENSE.txt</licenseUrl>
    <iconUrl>https://github.com/connellw/Firestorm/raw/master/nuget/icon.png</iconUrl>

    <dependencies>
      <dependency id="Firestorm.Core" version="$(FsVersion)" />
      <dependency id="Firestorm.Endpoints" version="$(FsVersion)" />
      <dependency id="Newtonsoft.Json" version="10.0" />
      <dependency id="Microsoft.Owin.Host.SystemWeb" version="3.0.1" />
    </dependencies>

    <references>
      <reference file="Firestorm.Owin.dll" />
    </references>
  </metadata>
  <files>
    <file src="..\..\src\Firestorm.Owin\bin\$(FsConfiguration)\net45\*.dll" target="lib\net45\"  />
  </files>
</package>