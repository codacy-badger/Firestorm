<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Firestorm.AspNetWebApi2</id>
    <version>$(FsVersion)</version>
    <title>Firestorm for ASP.NET Web API 2</title>
    <description>ASP.NET Web API 2.0 controller and helpers for Firestorm</description>
    <tags>firestorm rest api webapi2</tags>

    <authors>Connell Watkins</authors>
    <copyright>Copyright Â© Connell Watkins 2017</copyright>
    <projectUrl>https://github.com/connellw/Firestorm</projectUrl>
    <licenseUrl>https://github.com/connellw/Firestorm/blob/master/LICENSE.txt</licenseUrl>
    <iconUrl>https://github.com/connellw/Firestorm/raw/master/nuget/icon.png</iconUrl>

    <dependencies>
      <dependency id="Firestorm.Core" version="$(FsVersion)" />
      <dependency id="Firestorm.Endpoints" version="$(FsVersion)" />
      <dependency id="Newtonsoft.Json" version="10.0" />
      <dependency id="Microsoft.AspNet.WebApi" version="5.2.3" />
    </dependencies>

    <references>
      <reference file="Firestorm.AspNetWebApi2.dll" />
    </references>
  </metadata>
  <files>
    <file src="..\..\src\Firestorm.AspNetWebApi2\bin\$(FsConfiguration)\*.dll" target="lib\net45\"  />
    <!--file src="..\..\src\Firestorm.AspNetWebApi2\bin\$(FsConfiguration)\netstandard2.0\*.dll" target="lib\netstandard2.0\"  /-->
  </files>
</package>